        var $nImg = $cImg.next('.img img');
      }
      var $nText = $cText.next('.step');

      if(!$nImg[0]) {
        if($('.img picture', $box)[0]) {
          $nImg = $('.img picture', $box).first();
        } else {
          $nImg = $('.img img', $box).first();
        }
      }

      if(!$nText[0])
        $nText = $('.step', $box).first();

      if(!$nImg[0] || !$nText[0])
        return;

      swap($cImg, $nImg);
      swap($cText, $nText);
      return false;
    });

    function swap($cur, $next) {
      $cur.removeClass('current');
      $next.addClass('current');
    }
  });
</script>
